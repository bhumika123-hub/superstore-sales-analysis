    {
      "cell_type": "code",
      "source": [
        "from textblob import TextBlob\n",
        "\n",
        "def analyze_sentiment(text):\n",
        "    polarity = TextBlob(str(text)).sentiment.polarity\n",
        "    if polarity > 0:\n",
        "        return \"Positive\"\n",
        "    elif polarity < 0:\n",
        "        return \"Negative\"\n",
        "    else:\n",
        "        return \"Neutral\"\n",
        "\n",
        "df[\"Predicted_Sentiment\"] = df[\"Text\"].apply(analyze_sentiment)\n",
        "df.head()\n"
      ],
      "metadata": {
